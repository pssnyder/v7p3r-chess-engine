{
    "include": [
        "*.py"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__"
    ],
    "extraPaths": [
        "C:/Users/patss/AppData/Local/Programs/Python/Python312/Lib/site-packages"
    ],
    "ignore": [
        "**/*.ipynb"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.12",
    "typeCheckingMode": "basic",
    "useLibraryCodeForTypes": true,
